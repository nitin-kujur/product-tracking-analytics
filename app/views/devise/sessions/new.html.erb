<div class="container">
  <div class="row">
    <div class="col-md-4 col-md-offset-4"> 
      <div class="login-panel panel panel-default">
        <div class="panel-heading">
                        <h3 class="panel-title">Please Sign In</h3>
                    </div>
        <div class="panel-body">
  <div class="authform">
    <%= form_for(resource, :as => resource_name, :url => new_analytics_user_session_path(resource_name), :html => { :role => 'form'}) do |f| %>
      <%= devise_error_messages! %>
      <div class="form-group">
        <%= f.label :email %>
        <%= f.email_field :email, :autofocus => true, class: 'form-control' %>
      </div>
      <div class="form-group">
        <%= f.label :password %>
        <%= f.password_field :password, class: 'form-control' %>
      </div>
      <%= f.submit 'Sign in', :class => 'btn btn-lg btn-success btn-block' %>
      <% if devise_mapping.rememberable? -%>
        <div class="checkbox" style="width:150px">
          <label>
            <%= f.check_box :remember_me %> Remember me
          </label>
        </div>
      <% end -%>
  <% end %>
</div>
  <p>
  <!-- Download: -->
  <%#= link_to "variant", variant_index_path(format: "csv") %>
  <%#= link_to "lineitem", line_item_index_path(format: "csv") %>
  <%#= link_to "customer_tag", customer_tag_index_path(format: "csv") %>
  <%#= link_to "product_tag", product_tag_index_path(format: "csv") %>
  <%#= link_to "order_tag", order_tag_index_path(format: "csv") %>
  <%#= link_to "order", order_index_path(format: "csv") %>
  <%#= link_to "product", product_index_path(format: "csv") %>
  <%#= link_to "customer", customer_index_path(format: "csv") %>
  <%#= link_to "customer_rel", customer_customer_tag_index_path(format: "csv") %>
  <%#= link_to "product_rel", product_product_tag_index_path(format: "csv") %>
  <%#= link_to "order_re", order_order_tag_index_path(format: "csv") %>
</p>
</div>
</div>
</div>
</div>
</div>
</div>
